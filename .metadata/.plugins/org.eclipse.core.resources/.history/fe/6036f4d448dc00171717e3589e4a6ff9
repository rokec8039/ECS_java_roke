
public class YourSchedule {


public static void main(String[] args)
{
	
	String teacher1 = "Celniker";
	String teacher2 = "Mauro";
	String teacher3 = "Baum";
	String teacher4 = "Winburne";
	String class1 = "Math 1";
	String class2 = "ECS";
	String class3 = "Marketing";
	String class4 = "Spanish 3";
	
	
	
	
	System.out.println("--------------------------------------");
	System.out.println("|Per 1 |      " + class1 + "        |" + teacher1 + "|");
	System.out.println("|Per 2 |      " + class2 + "           |   " + teacher2 + "|");
	System.out.println("|Per 3 |      " + class3 + "     |    " + teacher3 + "|");
	System.out.println("|Per 3 |      " + class4 + "  |   " + teacher4 + "|");
	System.out.println("--------------------------------------");
	
	
	
	
	
	
	
	
}




}