public class YourInitials
{
	public static void main(String[] args)
	{
		System.out.println("     CCCCCCCCCC          RRRRRR");
		System.out.println("    CC        CC         R     R");
		System.out.println("   CC                    R     R");
		System.out.println("   CC                    RRRRRR");
		System.out.println("    CC        CC         R     R");
		System.out.println("     CCCCCCCCC           R      R");
	}

}