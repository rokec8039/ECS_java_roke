public class GoodFirstProgram
{
	public static void main(String[] args)
	{
		System.out.println("     CCCCCCCCC");
		System.out.println("    CC        C");
		System.out.println("   CC");
		System.out.println("   CC");
		System.out.println("    CC        C");
		System.out.println("     CCCCCCCCC");
	}

}